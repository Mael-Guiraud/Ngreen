\documentclass[border=10pt]{standalone}
\usepackage{tkz-graph}

\tikzset{
  LabelStyle/.style = { rectangle, rounded corners, draw,
                       font = \bfseries },
  VertexStyle/.append style = { inner sep=5pt,
                                font = \Large\bfseries},
  EdgeStyle/.append style = {-,double} }
\thispagestyle{empty}
\begin{document}
\begin{tikzpicture}
  \SetGraphUnit{5}
  \Vertex[x=0,y=0]{S1}
  \Vertex[x=0,y=2]{S2}
  \Vertex[x=0,y=4]{S3}
  
  \Vertex[x=6,y=0]{L1}
  \Vertex[x=6,y=2]{L2}
  \Vertex[x=6,y=4]{L3}
  
  \SetVertexNoLabel
  \Vertex[x=2,y=2]{A}
  \Vertex[x=4,y=2]{B}
  
  \Edge(S1)(A)
  \Edge(S2)(A)
  \Edge(S3)(A)
  
  \Edge(L1)(B)
  \Edge(L2)(B)
  \Edge(L3)(B)
  
  \Edge(A)(B)

\end{tikzpicture}

\end{document}